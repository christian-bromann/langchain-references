{
  "$schema": "./config-schema.json",
  "project": "langchain",
  "language": "typescript",
  "repo": "langchain-ai/langchainjs",
  "packages": [
    {
      "name": "langchain",
      "path": "libs/langchain",
      "entryPoints": ["auto"],
      "displayName": "LangChain",
      "versioning": {
        "tagPattern": "langchain@*",
        "maxVersions": 10,
        "pathOverrides": {
          "0.x": "langchain"
        }
      }
    },
    {
      "name": "@langchain/core",
      "path": "libs/langchain-core",
      "entryPoints": ["auto"],
      "displayName": "LangChain Core",
      "versioning": {
        "tagPattern": "@langchain/core@*",
        "maxVersions": 10,
        "pathOverrides": {
          "0.x": "langchain-core"
        }
      }
    },
    {
      "name": "@langchain/textsplitters",
      "path": "libs/langchain-textsplitters",
      "entryPoints": ["auto"],
      "displayName": "Text Splitters",
      "versioning": {
        "tagPattern": "@langchain/textsplitters@*",
        "maxVersions": 10,
        "pathOverrides": {
          "0.x": "libs/langchain-textsplitters"
        }
      }
    },
    {
      "name": "@langchain/mcp-adapters",
      "path": "libs/langchain-mcp-adapters",
      "entryPoints": ["auto"],
      "displayName": "MCP Adapters",
      "versioning": {
        "tagPattern": "@langchain/mcp-adapters@*",
        "maxVersions": 10,
        "pathOverrides": {
          "0.x": "libs/langchain-mcp-adapters"
        }
      }
    }
  ]
}
