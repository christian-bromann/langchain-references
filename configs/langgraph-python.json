{
  "$schema": "./config-schema.json",
  "project": "langgraph",
  "language": "python",
  "repo": "langchain-ai/langgraph",
  "packages": [
    {
      "name": "langgraph",
      "path": "libs/langgraph",
      "displayName": "LangGraph",
      "versioning": {
        "tagPattern": "*",
        "maxVersions": 10
      }
    },
    {
      "name": "langgraph_sdk",
      "path": "libs/sdk-py",
      "displayName": "LangGraph SDK",
      "versioning": {
        "tagPattern": "sdk==*",
        "maxVersions": 10
      }
    },
    {
      "name": "langgraph.checkpoint",
      "path": "libs/checkpoint",
      "displayName": "LangGraph Checkpoint",
      "versioning": {
        "tagPattern": "checkpoint==*",
        "maxVersions": 10
      }
    },
    {
      "name": "langgraph.checkpoint.sqlite",
      "path": "libs/checkpoint-sqlite",
      "displayName": "Checkpoint SQLite",
      "versioning": {
        "tagPattern": "checkpointsqlite==*",
        "maxVersions": 10
      }
    },
    {
      "name": "langgraph.checkpoint.postgres",
      "path": "libs/checkpoint-postgres",
      "displayName": "Checkpoint Postgres",
      "versioning": {
        "tagPattern": "checkpointpostgres==*",
        "maxVersions": 10
      }
    },
    {
      "name": "langgraph_cli",
      "path": "libs/cli",
      "displayName": "LangGraph CLI",
      "versioning": {
        "tagPattern": "cli==*",
        "maxVersions": 10
      }
    },
    {
      "name": "langgraph.prebuilt",
      "path": "libs/prebuilt",
      "displayName": "LangGraph Prebuilt",
      "versioning": {
        "tagPattern": "prebuilt==*",
        "maxVersions": 10
      }
    }
  ]
}
