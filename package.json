{
  "name": "langchain-reference-docs",
  "version": "0.1.0",
  "private": true,
  "description": "Unified API reference documentation platform for LangChain",
  "homepage": "https://reference.langchain.com",
  "license": "MIT",
  "author": "LangChain",
  "repository": {
    "type": "git",
    "url": "https://github.com/langchain-ai/langchain-reference-docs.git"
  },
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.local -- concurrently --names ir,web --prefix-colors cyan,magenta \"pnpm serve-ir\" \"pnpm --filter @langchain/reference-web dev\"",
    "dev:web": "turbo dev",
    "serve-ir": "tsx packages/ir-server/src/index.ts",
    "build": "turbo build",
    "build:ir": "tsx packages/build-pipeline/src/commands/build-ir.ts",
    "build:local": "node scripts/build-local.js",
    "pull-ir": "dotenv -e .env.local -- tsx packages/build-pipeline/src/commands/pull-ir.ts",
    "check-updates": "tsx packages/build-pipeline/src/commands/check-updates.ts",
    "sync-versions": "tsx packages/build-pipeline/src/commands/sync-versions.ts",
    "fetch-tarball": "tsx packages/build-pipeline/src/commands/fetch-tarball.ts",
    "update-pointers": "tsx packages/build-pipeline/src/commands/update-pointers.ts",
    "update-indexes": "tsx packages/build-pipeline/src/commands/update-indexes.ts",
    "upload-pointers": "dotenv -e .env.local -- tsx packages/build-pipeline/src/commands/upload-pointers.ts",
    "lint": "oxlint -c oxlint.json",
    "lint:fix": "oxlint -c oxlint.json --fix",
    "fmt": "oxfmt .",
    "fmt:check": "oxfmt --check .",
    "test": "turbo test",
    "test:e2e": "pnpm --filter @langchain/reference-web test:e2e",
    "test:e2e:wdio": "pnpm --filter @langchain/reference-web test:e2e:wdio",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "concurrently": "^9.1.0",
    "dotenv-cli": "^8.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.39.0",
    "tsx": "^4.21.0",
    "turbo": "^2.7.2",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "oxfmt",
      "oxlint -c oxlint.json"
    ],
    "*.{json,md,css,html,yaml,yml}": [
      "oxfmt"
    ]
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "packageManager": "pnpm@10.26.2"
}
