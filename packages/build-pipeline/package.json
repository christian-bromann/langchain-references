{
  "name": "@langchain/build-pipeline",
  "version": "0.1.0",
  "private": true,
  "description": "IR build pipeline for LangChain Reference Docs - extracts, transforms, and uploads API documentation",
  "files": [
    "src"
  ],
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./commands/*": {
      "types": "./src/commands/*.ts",
      "import": "./src/commands/*.ts"
    }
  },
  "scripts": {
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt .",
    "fmt:check": "oxfmt --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update": "vitest run --update",
    "build:ir": "tsx src/commands/build-ir.ts",
    "pull-ir": "tsx src/commands/pull-ir.ts",
    "check-updates": "tsx src/commands/check-updates.ts",
    "sync-versions": "tsx src/commands/sync-versions.ts",
    "fetch-tarball": "tsx src/commands/fetch-tarball.ts",
    "update-pointers": "tsx src/commands/update-pointers.ts",
    "update-indexes": "tsx src/commands/update-indexes.ts",
    "upload-pointers": "tsx src/commands/upload-pointers.ts"
  },
  "dependencies": {
    "@langchain/extractor-typescript": "workspace:*",
    "@langchain/ir-schema": "workspace:*",
    "@langchain/markdown-utils": "workspace:*",
    "@shikijs/rehype": "^3.6.0",
    "@vercel/blob": "^2.0.0",
    "commander": "^14.0.2",
    "p-limit": "^7.2.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "semver": "^7.6.0",
    "tar": "^7.5.2",
    "tinyglobby": "^0.2.14",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/semver": "^7.5.0",
    "@types/tar": "^6.1.13",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  }
}
