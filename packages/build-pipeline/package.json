{
  "name": "@langchain/build-pipeline",
  "version": "0.1.0",
  "description": "IR build pipeline for LangChain Reference Docs - extracts, transforms, and uploads API documentation",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./commands/*": {
      "types": "./src/commands/*.ts",
      "import": "./src/commands/*.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build:ir": "tsx src/commands/build-ir.ts",
    "pull-ir": "tsx src/commands/pull-ir.ts",
    "check-updates": "tsx src/commands/check-updates.ts",
    "sync-versions": "tsx src/commands/sync-versions.ts",
    "fetch-tarball": "tsx src/commands/fetch-tarball.ts",
    "update-pointers": "tsx src/commands/update-pointers.ts"
  },
  "dependencies": {
    "@langchain/extractor-typescript": "workspace:*",
    "@langchain/ir-schema": "workspace:*",
    "@vercel/blob": "^2.0.0",
    "commander": "^14.0.2",
    "semver": "^7.6.0",
    "tar": "^7.5.2",
    "tinyglobby": "^0.2.14"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/semver": "^7.5.0",
    "@types/tar": "^6.1.13",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}

