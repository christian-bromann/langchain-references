{
  "name": "@langchain/extractor-typescript",
  "version": "0.1.0",
  "description": "TypeScript API extractor for LangChain reference docs",
  "private": true,
  "bin": {
    "extract-typescript": "./src/cli.ts"
  },
  "files": [
    "src"
  ],
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt .",
    "fmt:check": "oxfmt --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "tinyglobby": "^0.2.14",
    "typedoc": "^0.28.15"
  },
  "devDependencies": {
    "@langchain/ir-schema": "workspace:*",
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@langchain/ir-schema": "workspace:*"
  }
}
