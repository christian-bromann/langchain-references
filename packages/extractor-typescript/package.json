{
  "name": "@langchain/extractor-typescript",
  "version": "0.1.0",
  "description": "TypeScript API extractor for LangChain reference docs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "extract-typescript": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "tinyglobby": "^0.2.14",
    "typedoc": "^0.28.15"
  },
  "devDependencies": {
    "@langchain/ir-schema": "workspace:*",
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@langchain/ir-schema": "workspace:*"
  }
}

