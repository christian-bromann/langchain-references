{
  "name": "@langchain/related-docs-scanner",
  "version": "0.1.0",
  "private": true,
  "description": "Scans LangChain docs repository for symbol imports to create related documentation links",
  "files": [
    "src"
  ],
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@langchain/ir-schema": "workspace:*",
    "gray-matter": "^4.0.3",
    "simple-git": "^3.27.0",
    "tinyglobby": "^0.2.14"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  }
}
